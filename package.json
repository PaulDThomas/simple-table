{
  "name": "@asup/simple-table",
  "version": "1.4.1",
  "description": "REACT table, because I wanted one that took an array of objects as an input.",
  "author": "Paul Thomas <@PaulDThomas>",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaulDThomas/simple-table.git"
  },
  "keywords": [
    "react",
    "typescript",
    "simple",
    "array-of-objects",
    "table"
  ],
  "bugs": {
    "url": "https://github.com/PaulDThomas/simple-table/issues"
  },
  "homepage": ".",
  "main": "dist/cjs/main.js",
  "module": "dist/esm/main.js",
  "types": "dist/simple-table.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky",
    "start": "start.bat",
    "test": "jest --maxWorkers=1",
    "test-watch": "jest --watch --collectCoverage=true --maxWorkers=4",
    "test-coverage": "jest --collectCoverage=true --maxWorkers=4",
    "build": "parcel build src/main.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.7.0",
    "@parcel/packager-ts": "^2.8.2",
    "@parcel/transformer-typescript-types": "^2.8.2",
    "@parcel/transformer-webmanifest": "^2.7.0",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^15.0.2",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.2",
    "@types/node": "^20.12.7",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-jest": "^28.2.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.10",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.3.1",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^15.2.0",
    "parcel": "^2.7.0",
    "prettier": "^3.0.3",
    "process": "^0.11.10",
    "ts-jest": "^29.0.3",
    "ts-jest-mock-import-meta": "^1.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "@parcel/transformer-js": {
    "inlineEnvironment": [
      "REACT_APP_*",
      "npm_package_*"
    ]
  },
  "@parcel/transformer-css": {
    "cssModules": {
      "pattern": "ast-[name]-[hash]-[local]",
      "dashedIndents": true
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
